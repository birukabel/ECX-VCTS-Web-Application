@model ECX.VCTS.ViewModels.ViolationReporterViewModel
@{
    ViewData["Title"] = "Report Violation";
    ViewBag.Subtitle = "Fill Form and Send Violation Case";
}

<form asp-action="ReportViolation" asp-controller="ViolationReporter" method="post" enctype="multipart/form-data">


    <div class="container-fluid">

        <div class="row">

            <div class="col-md-1"></div>
            <div class="col-md-3">
                <div class="form-group">
                    <label asp-for="violationCases.ViolationType" class="control-label"></label>
                    @Html.DropDownListFor(m => m.violationCases.ViolationType, Model.ViolationTypes,
                   "Choose Violation Type", new { @id = "ReportViolationTypeName", @class = "form-control" })
                    <span asp-validation-for="violationCases.ViolationType" class="text-danger"></span>
                </div>
            </div>

            <div class="col-lg-4" id="warehousecol">
                <div class="form-group">
                    @Html.LabelFor(m => m.violationCases.ExchangeCenterName)
                    @Html.DropDownListFor(m => m.violationCases.ExchangeCenterName, Model.Warehouses,
                    "Choose center", new { @class = "form-control", @id = "warehouses" })
                </div>
            </div>
            <div class="col-lg-4" id="centerscol" hidden>
                <div class="form-group">
                    @Html.LabelFor(m => m.violationCases.ExchangeCenterName)
                    @Html.DropDownListFor(m => m.violationCases.ExchangeCenterName, Model.Centers,
                   "Choose center", new { @class = "form-control", @id = "centers" })
                </div>
            </div>

            <div class="col-md-5"></div>

        </div>

        <div class="row">
            <div class="col-md-1"></div>

            <div class="col-md-3">
                <div class="form-group">
                    <label asp-for="violationCases.ViolatorName" class="control-label"></label>
                    <input asp-for="violationCases.ViolatorName" class="form-control allegedviolator" id="violatorname" />
                    <span asp-validation-for="violationCases.ViolatorName" class="text-danger"></span>
                </div>
            </div>

            <div class="col-md-3" id="tradeviolationdatediv">
                <div class="form-group">
                    <label asp-for="tradeModel.ViolationDate" class="control-label"></label>
                    <input asp-for="tradeModel.ViolationDate" class="form-control" id="datepicker1" value="mm/dd/yyyy" />
                    <span asp-validation-for="tradeModel.ViolationDate" class="text-danger"></span>
                </div>
            </div>

            <div class="col-md-3" id="surveilviolationdatediv" hidden>
