@section scripts{

    <script type="text/javascript">

        $(document).ready(function () {
                document.getElementById('warehouses').disabled = true;
             var Extensions = ['png','jpeg', 'jpg','gif','docx', 'doc','xlsx', 'xls','txt','pdf','rar','zip'];
                var text = $('#ReportViolationTypeName').find('option:selected').text();
                if (text != "" && text != "Choose Violation Type")
                    $('#sendbut').show();
                if (text == "Quality") {
                    $('#tradeviolationdatediv').hide();
                    $('#surveilviolationdatediv').hide();
                    $('#arrivaldatediv').show();
                    $('#qualityparametersdiv').show();
                    $('#qualitysubcatdiv').show();
                    $('#tradeparametersdiv').hide();
                    $('#survparametersdiv').hide();
                    $('#othersviolationdatediv').hide();
